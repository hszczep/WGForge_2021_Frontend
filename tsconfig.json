{
  "compilerOptions": {
    "outDir": "./dist/",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "module": "es6",
    "target": "es6",
    "allowJs": true,
    "typeRoots": [
      "custom_typings"
    ],
    "lib": [ "DOM", "ES6", "DOM.Iterable", "ScriptHost" ],
  },
  "include": ["src/**/*", "global.d.ts"],
  "exclude": ["dist", "node_modules"]
}